#include <iostream>
#include <string>
using namespace std;

int main()
{

	//vector <char> v;


	string word;

	cout << "Word: ";
	getline(cin,word);

	for (int i = 0; i <= word.size(); i++)

	{
		if (word[i] >= 97 && word[i] <= 122)
		{
			word[i] = word[i] - 32;
		}
	}
	cout << "Word in Uppercase : " << word;
	return 0;
}
